{
  "name": "vue-float-menu",
  "version": "1.9.0",
  "description": "smart floating menu for Vue 3",
  "license": "MIT",
  "keywords": [
    "menu",
    "float-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-float-menu"
  },
  "author": {
    "name": "Prabhu Murthy",
    "url": "https://www.prabhumurthy.com",
    "email": "prabhu.m.murthy@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "rollup": "rimraf ./dist && rollup -c",
    "lint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.scss",
    "lint:all": "yarn lint && yarn lint:css",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npx stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-sucrase": "^4.0.1",
    "@rollup/plugin-typescript": "^8.2.5",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.8",
    "rollup": "^2.58.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.2",
    "vue": "^3.2.8",
    "vue-float-menu": "^1.9.0"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "vue": "^3.0.4"
  },
  "main": "dist/vue-float-menu.js",
  "module": "dist/vue-float-menu.es.js",
  "umd": "dist/vue-float-menu.umd.js",
  "files": [
    "dist"
  ]
}
