{
  "name": "vue-float-menu",
  "version": "2.0.1",
  "description": "smart floating menu for Vue 3",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "menu",
    "float-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-float-menu"
  },
  "author": {
    "name": "Prabhu Murthy",
    "url": "https://www.prabhumurthy.com",
    "email": "prabhu.m.murthy@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build && vue-tsc --build tsconfig.build.json --emitDeclarationOnly",
    "build:types": "vue-tsc --build tsconfig.build.json --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint --no-error-on-unmatched-pattern \"src/**/*.{js,ts,jsx,tsx,mjs,cjs}\"",
    "lint:js:fix": "eslint --ext .js,.ts . --fix",
    "lint:css": "stylelint \"src/**/*.{vue,scss,css}\"",
    "lint:css:fix": "stylelint \"src/**/*.{vue,scss,css}\" --fix",
    "lint:all": "pnpm lint && pnpm lint:css && pnpm type-check",
    "lint:fix:all": "pnpm lint:fix && pnpm lint:css:fix",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.vue\" \"src/**/*.scss\" \"src/**/*.ts\" \"src/**/*.js\"",
    "clean": "pnpm format && pnpm lint:js:fix && pnpm lint:css:fix"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.18",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vue-scoped-css": "^2.11.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "terser": "^5.43.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.3.0 || ^4.0.0-0"
  },
  "main": "dist/vue-float-menu.cjs",
  "module": "dist/vue-float-menu.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-float-menu.js",
      "require": "./dist/vue-float-menu.cjs",
      "default": "./dist/vue-float-menu.js"
    },
    "./style.css": "./dist/vue-float-menu.css"
  },
  "files": [
    "dist"
  ],
  "unpkg": "dist/vue-float-menu.umd.js",
  "jsdelivr": "dist/vue-float-menu.umd.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=9"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
