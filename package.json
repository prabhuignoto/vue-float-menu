{
  "name": "vue-float-menu",
  "version": "1.9.1",
  "description": "smart floating menu for Vue 3",
  "license": "MIT",
  "keywords": [
    "menu",
    "float-menu",
    "vue-menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prabhuignoto/vue-float-menu"
  },
  "author": {
    "name": "Prabhu Murthy",
    "url": "https://www.prabhumurthy.com",
    "email": "prabhu.m.murthy@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "rollup": "rimraf ./dist && rollup -c",
    "lint": "eslint src/**/*.vue",
    "lint:css": "stylelint src/**/*.scss --custom-syntax postcss-scss",
    "lint:all": "yarn lint && yarn lint:css",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "npx stylelint src/**/*.scss --fix",
      "git add"
    ],
    "src/**/*.vue": [
      "eslint src/**/*.vue --fix",
      "git add"
    ]
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-beep": "^0.2.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-sucrase": "^4.0.2",
    "@rollup/plugin-typescript": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "rollup": "^2.70.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.9",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^23.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vue": "^3.2.31",
    "vue-float-menu": "^1.9.1"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "vue": "^3.0.4"
  },
  "main": "dist/vue-float-menu.js",
  "module": "dist/vue-float-menu.es.js",
  "umd": "dist/vue-float-menu.umd.js",
  "files": [
    "dist"
  ]
}
